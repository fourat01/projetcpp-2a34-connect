   <string notr="true">/* ===== Global ===== */
QWidget {
    background-color: #E3F2FD;   /* very light blue background */
    color: #1B5E20;              /* dark green text */
    font-family: &quot;Segoe UI&quot;;
    font-size: 12pt;
}

/* ===== QPushButton ===== */
QPushButton {
    background-color: #4CAF50;   /* green */
    color: white;
    border: 2px solid #388E3C;
    border-radius: 8px;
    padding: 6px 12px;
}
QPushButton:hover {
    background-color: #2196F3;   /* blue on hover */
}
QPushButton:pressed {
    background-color: #1976D2;   /* darker blue when clicked */
}

/* ===== QLineEdit / QTextEdit ===== */
QLineEdit, QTextEdit {
    background-color: #FFFFFF;
    border: 2px solid #2196F3;   /* blue border */
    border-radius: 6px;
    padding: 4px;
    color: #1B5E20;              /* dark green text */
}
QLineEdit:focus, QTextEdit:focus {
    border: 2px solid #4CAF50;   /* green border on focus */
}

/* ===== QLabel ===== */
QLabel {
    color: #0D47A1;              /* deep blue text */
    font-weight: bold;
}

/* ===== QComboBox ===== */
QComboBox {
    background-color: #FFFFFF;
    border: 2px solid #4CAF50;
    border-radius: 6px;
    padding: 4px;
}
QComboBox:hover {
    border: 2px solid #2196F3;
}

/* ===== QTableWidget / QTreeWidget ===== */
QHeaderView::section {
    background-color: #2196F3;   /* blue header */
    color: white;
    padding: 6px;
    border: none;
}
QTableWidget, QTreeWidget {
    background-color: #FFFFFF;
    border: 2px solid #4CAF50;
    gridline-color: #2196F3;
}

/* ===== QScrollBar ===== */
QScrollBar:vertical {
    background: #E3F2FD;
    width: 12px;
}
QScrollBar::handle:vertical {
    background: #4CAF50;
    border-radius: 6px;
}
QScrollBar::handle:vertical:hover {
    background: #2196F3;
}
QScrollBar:horizontal {
    background: #E3F2FD;
    height: 12px;
}
QScrollBar::handle:horizontal {
    background: #4CAF50;
    border-radius: 6px;
}
QScrollBar::handle:horizontal:hover {
    background: #2196F3;
}

/* ===== QProgressBar ===== */
QProgressBar {
    border: 2px solid #2196F3;
    border-radius: 8px;
    text-align: center;
}
QProgressBar::chunk {
    background-color: #4CAF50;   /* green progress */
    border-radius: 8px;
}

/* ===== QTabWidget ===== */
QTabWidget::pane {
    border: 2px solid #2196F3;   /* blue border around content */
    border-radius: 8px;
    background: #FFFFFF;
}
QTabBar::tab {
    background: #BBDEFB;         /* light blue tab */
    color: #1B5E20;              /* dark green text */
    border: 1px solid #2196F3;
    border-radius: 6px;
    padding: 6px 12px;
    margin-right: 4px;
}
QTabBar::tab:selected {
    background: #4CAF50;         /* green when selected */
    color: white;
}
QTabBar::tab:hover {
    background: #2196F3;         /* blue on hover */
    color: white;
}
QDateEdit {
    background-color: #FFFFFF;
    border: 2px solid #2196F3;   /* blue border */
    border-radius: 6px;
    padding: 4px;
    color: #1B5E20;              /* dark green text */
}

QDateEdit:focus {
    border: 2px solid #4CAF50;   /* green border when focused */
}

QCalendarWidget QAbstractItemView:enabled {
    background-color: #FFFFFF;
    selection-background-color: #4CAF50; /* green selected day */
    selection-color: white;
}

QListWidget {
    background-color: #FFFFFF;
    border: 2px solid #2196F3;   /* blue border */
    border-radius: 6px;
    padding: 4px;
    color: #1B5E20;              /* dark green text */
}

QListView::item {
    padding: 6px;
}

QListView::item:selected {
    background-color: #4CAF50;   /* green when selected */
    color: white;
}

QListView::item:hover {
    background-color: #2196F3;   /* blue when hovered */
    color: white;
}
/* Fond général de l'onglet */
QTabWidget, QTabWidget::pane {
    background-color: #FFFFFF; /* blanc */
    border: 1px solid #BFA2DB; /* bordure mauve claire pour délimiter */
    border-radius: 5px;
}

/* Fond des onglets (onglets non sélectionnés) */
QTabBar::tab {
    background-color: #D6C1E9; /* mauve pastel */
    color: #4B0082; /* violet foncé */
    padding: 8px 15px;
    border: 1px solid #BFA2DB;
    border-bottom: none;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    margin-right: 2px;
}

/* Onglet sélectionné */
QTabBar::tab:selected {
    background-color: #FFFFFF; /* blanc */
    color: #4B0082;
    border: 1px solid #9D65C9;
    border-bottom: 1px solid #FFFFFF; /* pour fusionner avec le panneau */
}

/* Onglet au survol */
QTabBar::tab:hover {
    background-color: #9D65C9;
    color: #FFFFFF;
}
/* Bouton poussoir principal */
QPushButton {
    background-color: #9D65C9;       /* Mauve vif */
    color: #FFFFFF;                  /* Texte blanc */
    border: 2px solid #4B0082;      /* Bordure violet foncé */
    border-radius: 10px;            /* Bords arrondis */
    padding: 10px 20px;             /* Espacement interne */
    font-size: 16px;                /* Taille du texte */
    font-weight: bold;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

/* Bouton poussoir au survol */
QPushButton:hover {
    background-color: #7A3EA1;       /* Mauve plus foncé */
}

/* Bouton poussoir enfoncé (clic) */
QPushButton:pressed {
    background-color: #5B2E80;       /* Mauve très foncé */
    border-color: #3B1E59;           /* Bordure plus foncée */
}
QHeaderView::section {
    background-color: white;
    color: black;   /* texte en noir pour qu’il reste lisible */
    font-weight: bold;
    border: 1px solid #dcdcdc; /* optionnel, pour séparer les colonnes */
}
QTableWidget {
    border: 2px solid white;   /* contour blanc */
    gridline-color: gray;      /* couleur des lignes intérieures (optionnel) */
}





